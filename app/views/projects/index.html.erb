<div class="page-header">

  <h1>Projects
  <%= link_to 'Create Project', new_project_path, class:"btn btn-info btn-large, pull-right" %></h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>
        Project
      </th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= link_to(project.name, project_path(project)) %></td>
      </tr>
    <%end%>
  </tbody>
</table>
<hr>
