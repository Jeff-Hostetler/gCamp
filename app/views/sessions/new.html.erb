<%= form_for(:session, url: login_path) do |f| %>
  

    <div class="form-group">
      <label for="due" class="form-control, col-sm-2 control-label">Email</label>
      <div class="col-sm-10">
        <%= f.email_field :email, type:"email", class:"form-control, col-sm-5" %>
      </div>
    </div>

    <div class="form-group">
      <label for="due" class="form-control, col-sm-2 control-label">Password</label>
      <div class="col-sm-10">
        <%= f.password_field :password, type:"password", class:"form-control, col-sm-5" %>
      </div>
    </div>

    <br></br>
    <div class="form-group">
      <div class="col-sm-offset-2 col-sm-10">
        <%= f.submit "Sign In" %>
      </div>
    </div>
  <br></br>


  </form>

<% end %>
